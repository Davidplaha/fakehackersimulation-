{
  "name": "simdeck-trace-video",
  "private": true,
  "version": "0.0.0",
  "description": "Remotion project for SimDeck trace (simulation-only) video assets.",
  "scripts": {
    "studio": "remotion studio",
    "render:globalnet:mp4": "remotion render GlobalNetwork ../assets/videos/global-network.mp4 --codec h264 --overwrite",
    "render:globalnet:webm": "remotion render GlobalNetwork ../assets/videos/global-network.webm --codec vp8 --overwrite",
    "render:globalnet": "npm run render:globalnet:mp4",
    "render:critical:mp4": "remotion render CriticalDownload ../assets/videos/critical-download.mp4 --codec h264 --overwrite",
    "render:critical:webm": "remotion render CriticalDownload ../assets/videos/critical-download.webm --codec vp8 --overwrite",
    "render:critical": "npm run render:critical:mp4",
    "render:trace:mp4": "remotion render TraceSim ../assets/videos/trace-sim.mp4 --codec h264 --overwrite",
    "render:trace:webm": "remotion render TraceSim ../assets/videos/trace-sim.webm --codec vp8 --overwrite",
    "render:trace": "npm run render:trace:mp4"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remotion": "^4.0.0"
  },
  "devDependencies": {
    "@remotion/cli": "^4.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.0.0"
  }
}
